	<Device id="ctrl">
		<registerAccess>MemBus</registerAccess>
		<baseAddress>0x20000000</baseAddress>
		<!-- Control of the firmware pipeline (and CoBo ...) -->
		<Register id="pipeCtrl">
			<!-- Enables the Power PC interrupt. -->
			<BitField id="intEnable">
				<offset>0</offset>
			</BitField>
			<!-- Alias for "intEnable". Deprecated since we are doing one CPU interrupt per CoBo instead of one per AsAd. Kept for backward-compatibility. -->
			<BitField id="intEnable_asad0">
				<offset>0</offset>
			</BitField>
			<!-- Specific to 1 AGET. -->
			<BitField id="backpressure">
				<offset>1</offset>
			</BitField>
			<!-- Resets all the CoBo logic if set to 1 and then to 0. In particular, resets the circular buffer write pointer. -->
			<BitField id="reset">
				<offset>2</offset>
			</BitField>
			<!-- Allows the Slow Control to override the state of SCW and SCR (normally changed only by the firmware) for debugging purposes. If set, enabled the SCR and SCW bit fields.-->
			<BitField id="ScrScwOverride">
				<offset>3</offset>
			</BitField>
			<!-- Software SCA Read. -->
			<BitField id="SCR">
				<offset>4</offset>
			</BitField>
			<!-- Software SCA Write. -->
			<BitField id="SCW">
				<offset>5</offset>
			</BitField>
			<!-- Deprecated. Override of the AGET fast control (hit registers readout). -->
			<BitField id="hitRegisterOverride">
				<offset>6</offset>
			</BitField>
			<!-- If set, allows the firmware state machine to run.-->
			<BitField id="firmwareEnable">
				<offset>7</offset>
			</BitField>
			<!-- Set to 1 to initiate CoBo deserializer deskew sequence. -->
			<BitField id="adcDeskew">
				<offset>8</offset>
			</BitField>
			<!-- Specific to 1 AGET firmware. Switches between 16-bit (0) and 32-bit (1) frame types.-->
			<BitField id="frameType">
				<width>1</width>
				<offset>9</offset>
			</BitField>
			<!-- Enables the zero suppression mode. -->
			<BitField id="zeroSuppressionEnable">
				<offset>10</offset>
			</BitField>
			<!-- Enables the test mode, where the frames produced are filled with an incremental counter instead of the actual AGet data. -->
			<BitField id="testMode">
				<offset>11</offset>
			</BitField>
			<!-- Enables the partial readout mode.-->
			<BitField id="partialReadOut">
				<offset>12</offset>
			</BitField>
			<!-- Set to 1 to initiate the CoBo deserializer calibration sequence. -->
			<BitField id="bitSlipCalibrate">
				<offset>13</offset>
			</BitField>
			<!--If set, enables 2p mode and reads data accordingly. -->
			<BitField id="modeEnable_2p">
				<offset>14</offset>
			</BitField>
			<!-- Specific to 1 AGET firmware. Creates frames of multiplicity data.-->
			<BitField id="multiplicityReadOut">
				<width>2</width>
				<offset>15</offset>
			</BitField>
			<!-- Selects auto-calibrated delay (1) or manual delay (0).-->
			<BitField id="SCRAutoDelayEnable">
				<offset>17</offset>
			</BitField>
			<!-- Set to 1 for auto-calibrating the delay between CoBo and AsAd. Switch Capacitor Array.-->
			<BitField id="SCRAutoDelayCalibrate">
				<offset>18</offset>
			</BitField>
			<!-- Allows, if set, to modify hit pattern before readout. Modified hit pattern is OR'ed and AND'ed with registers 'readAlways' and ' 'readIfHit' respectively. Value: 0 or 1.-->
			<BitField id="writeHitRegisterMaskEnable">
				<offset>19</offset>
			</BitField>
			<!-- When set, the four noise channels (Fixed Pattern Noise channels) are always read, even in partial readout mode. Value: 1 or 0. -->
			<BitField id="isFPNRead">
				<offset>20</offset>
			</BitField>
			<!-- When set, a moving average is automatically subtracted from each AGET's raw multiplicity value. -->
			<BitField id="MultSubtractAverage">
