			<BitField id="Readout_mode">
				<width>1</width>
				<offset>14</offset>
			</BitField>
			<!-- Bit FPN readout: if set, forces the readout of the FPN channels (e.g. even in partial readout mode).-->
			<BitField id="FPNreadout">
				<width>1</width>
				<offset>15</offset>
			</BitField>
